{
    "version": 2,
    "builds": [
      {
        "src": "Server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "client/build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Server/server.js"
      }
    ]
  }
  
